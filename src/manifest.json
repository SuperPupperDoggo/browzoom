{
  "manifest_version": 2,

  "name": "Browzoom",
  "version": "0.1.0",
  "description": "Automatically show the join from browser button on zoom",
  "icons": { "16": "browzoom-16.png",
           "48": "browzoom-48.png",
          "128": "browzoom-128.png" },

  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["https://*.zoom.us/*"]
  }]

}
